<%- include('../header.ejs') %>

<br><br>

    <table class="table table-bordered table-striped">
        <tr class="bg-secondary">
            <th>Serial.no</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone Number</th>
            <th>gender</th>
            <th>DOB</th>
            <th>Address</th>
            <th>College</th>
            <th>Course</th>
            <th>Branch</th>
            <th>Status</th>
            <th>Comment</th>
            <th>ACtion</th>
        </tr>
        <% i=1 %>

            <% f.forEach((b)=>{ %>
                <tr class="bg-warning">
               
                    <td>
                        <%= i++ %>
                    <td>
                        <%= b.name %>
                    </td>
                    <td>
                        <%= b.email %>
                    </td>
                    <td>
                        <%= b.phone %>
                    </td>
                    <td>
                        <%= b.gender %>
                    </td>

                    <td>
                        <%= b.dob %>
                    </td>
                    <td>
                        <%= b.address %>
                    </td>
                    <td>
                        <%= b.college %>
                    </td>
                    <td>
                        <%= b.course %>
                    </td>
                    <td>
                        <%= b.branch %>
                    </td>
                    <td>
                        <%= b.status%>
                    </td>
                    <td>
                        <%= b.comment%>
                    </td>
                    <td>
                      
                        <a href="/admin/formedit/<%=b._id%>" class="btn btn-warning"><i
                                class="fa-regular fa-pen-to-square"></i></a>
                    
                                <a href="/admin/formdelete/<%= b._id %>" class="btn btn-danger"><i
                                    class="fa-solid fa-trash"></i></a>
                            </td>

                </tr>
                    
                <% }) %>
    </table>
    <br><br>
    <a href="/home" class="btn btn-info">Submit</a>
    <br><br>
    <%- include('../footer.ejs') %>